{
  "name": "@tetrascience-npm/tetrascience-react-ui",
  "version": "0.3.0",
  "description": "TetraScience React UI",
  "type": "module",
  "author": "TetraScience",
  "license": "Apache-2.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/server.d.ts",
        "default": "./dist/esm/server.js"
      },
      "require": {
        "types": "./dist/server.d.ts",
        "default": "./dist/cjs/server.js"
      }
    },
    "./index.css": "./dist/index.css",
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "vitest run --project=unit",
    "test:watch": "vitest --project=unit",
    "test:coverage": "vitest run --project=unit --coverage",
    "test:storybook": "vitest run --project=storybook",
    "test:storybook:watch": "vitest --project=storybook",
    "test:storybook:ci": "vitest run --project=storybook --reporter=junit --outputFile=test-results/storybook-junit.xml",
    "test:all": "vitest run",
    "build": "rm -rf dist && rollup -c --bundleConfigAsCjs",
    "storybook": "pms build && storybook dev -p 6006",
    "build-storybook": "pms build && storybook build",
    "prepare": "husky",
    "prepublishOnly": "pms run build"
  },
  "dependencies": {
    "@dagrejs/dagre": "^1.1.4",
    "@monaco-editor/react": "^4.7.0",
    "@tetrascience-npm/ts-connectors-sdk": "^3.1.1",
    "@types/plotly.js": "^3.0.0",
    "@types/react-plotly.js": "^2.6.3",
    "@xyflow/react": "^12.6.4",
    "axios": "^1.9.0",
    "elkjs": "^0.10.0",
    "immer": "^10.1.1",
    "monaco-editor": "^0.52.2",
    "monaco-themes": "0.4.7",
    "plotly.js": "^3.0.1",
    "plotly.js-dist": "^2.35.2",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-markdown": "^10.1.0",
    "react-plotly.js": "^2.6.0",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "styled-components": "^6.1.18",
    "use-immer": "^0.11.0"
  },
  "peerDependencies": {
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@storybook/addon-essentials": "^8.5.6",
    "@storybook/addon-interactions": "^8.5.6",
    "@storybook/addon-links": "^8.5.6",
    "@storybook/blocks": "^8.5.6",
    "@storybook/experimental-addon-test": "8.6.14",
    "@storybook/react": "^8.5.6",
    "@storybook/react-vite": "^8.5.6",
    "@storybook/test": "^8.5.6",
    "@types/jsdom": "^27",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-syntax-highlighter": "^15",
    "@types/rollup-plugin-peer-deps-external": "^2",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/browser": "^2.1.0",
    "@vitest/coverage-v8": "^2.1.0",
    "eslint": "^9.9.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-storybook": "^0.11.2",
    "globals": "^15.9.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^15.2.0",
    "playwright": "^1.58.1",
    "pmswitch": "^1.0.8",
    "rollup": "^4.41.0",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "sass": "^1.89.0",
    "storybook": "^8.5.6",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vitest": "^2.1.0"
  },
  "resolutions": {
    "jackspeak": "2.1.1",
    "typescript": "5.6.3"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "packageManager": "yarn@4.12.0",
  "workspaces": [
    "examples/*"
  ],
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tetrascience/ts-lib-ui-kit.git"
  },
  "keywords": [
    "tetrascience",
    "react",
    "ui"
  ],
  "bugs": {
    "url": "https://github.com/tetrascience/ts-lib-ui-kit/issues"
  },
  "homepage": "https://github.com/tetrascience/ts-lib-ui-kit#readme"
}
